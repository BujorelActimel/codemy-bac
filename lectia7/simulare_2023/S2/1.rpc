citeste n //  (numÄƒr natural)
x<-5
repeta
    cn<-n
    n<-0
    p<-1
    repeta
        c<-cn%10
        daca c=x atunci 
            c<-5-c
        sf
        n<-c*p+n
        cn<-[cn/10]
        p<-p*10
    pana cand cn=0
    scrie n
    x<-x-1
pana cand x=0
scrie n

